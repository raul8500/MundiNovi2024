        /* Estilos para la lista desplegable */
        #listaProductos {
            max-height: 300px;
            overflow-y: auto;
            position: absolute;
            background: white;
            border: 1px solid #ddd;
            z-index: 1000;
        }
        #listaProductos .list-group-item {
            cursor: pointer;
        }
        #sugerencias {
            position: absolute;
            border: 1px solid #ddd;
            background: #fff;
            max-height: 200px;
            overflow-y: auto;
            z-index: 1000;
        }
        #sugerenciasEdit {
            position: absolute;
            border: 1px solid #ddd;
            background: #fff;
            max-height: 200px;
            overflow-y: auto;
            z-index: 1000;
        }
        .sugerencia {
            padding: 10px;
            cursor: pointer;
        }
        .sugerencia.seleccionado {
            background: #ddd;
        }
        .total {
            font-weight: bold;
        }


         /* Estilo para reducir el tamaño de las sugerencias */
        #suggestions {
            max-height: 200px;
            overflow-y: auto;
            border: 1px solid #ddd;
            background-color: white;
            padding: 0;
            list-style-type: none;
            width: 300px;
        }

        #suggestions li {
            padding: 10px;
            cursor: pointer;
        }

        #suggestions li.active {
            background-color: #007bff;
            color: white;
        }

        .suggestions {
        border: 1px solid #ccc;
        max-height: 150px;
        overflow-y: auto;
        position: absolute;
        background: #fff;
        width: 300px;
        }
        .suggestion-item {
        padding: 8px;
        cursor: pointer;
        }
        .suggestion-item:hover, .suggestion-item.selected {
        background-color: #f0f0f0;
        }
        
    </style>
